<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="circleimageview">
        <attr name="border_outside_color" format="color" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_thickness" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="TabViews">
        <attr name="bottom_labels" format="reference" />
    </declare-styleable>

    <declare-styleable name="LabelCheckbox">
        <attr name="checkbox_text" format="string"/>
        <attr name="checkbox_paddingTop" format="dimension"/>
        <attr name="checkbox_paddingBottom" format="dimension"/>
        <attr name="checkbox_paddingLeft" format="dimension"/>
        <attr name="checkbox_paddingRight" format="dimension"/>
        <attr name="checkbox_background" format="reference"/>
        <attr name="checkbox_textSize" format="dimension"/>
        <attr name="checkbox_textColor" format="color"/>
        <attr name="checkbox_checked" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="HomeSearchView">
        <attr name="search_hint" format="string"/>
        <attr name="search_show_scan" format="boolean"/>
    </declare-styleable>
</resources>
